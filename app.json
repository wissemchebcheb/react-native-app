{
  "expo": {
    "name": "global-project",
    "slug": "global-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/instagram.png",
    "splash": {
      "image": "./assets/instagram.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffe5f1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/instagram.png",
        "backgroundColor": "#ffe5f1"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
